{
  "name": "url-shortener-rest",
  "version": "1.0.0",
  "description": "REST API for URL Shortener - Academic comparison with gRPC implementation",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "lint": "eslint src/ --ext .js",
    "docker:build": "docker build -t url-shortener-rest .",
    "docker:run": "docker run -p 8083:8083 url-shortener-rest"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "joi": "^17.9.2",
    "uuid": "^9.0.0",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^2.4.2",
    "crypto": "^1.0.1",
    "validator": "^13.9.0",
    "geoip-lite": "^1.4.7",
    "useragent": "^2.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.1",
    "eslint": "^8.45.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}