    "qrcode": "^1.5.3",
    "multer": "^1.4.5-lts.1",
    "joi": "^17.9.2",
    "uuid": "^9.0.0",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^2.4.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.1",
    "eslint": "^8.45.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}