{
  "name": "qr-generator-rest",
  "version": "1.0.0",
  "description": "REST API for QR Code Generator - Academic comparison with gRPC implementation",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "lint": "eslint src/ --ext .js",
    "docker:build": "docker build -t qr-generator-rest .",
    "docker:run": "docker run -p 8082:8082 qr-generator-rest"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
